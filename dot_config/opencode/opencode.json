{
  "$schema": "https://opencode.ai/config.json",
  "model": "openrouter/qwen/qwen3-coder",
  "provider": {
    "openrouter": {
      "models": {
        "qwen/qwen3-coder": {
          "options": {
            "provider": {
              "sort": "throughput",
              "quantizations": [
                "fp8"
              ],
              "ignore": [
                "Novita"
              ]
            }
          }
        },
        "z-ai/glm-4.5": {
          "options": {
            "provider": {
              "order": [
                "z-ai/fp8",
                "deepinfra/fp8",
                "novita"
              ],
              "ignore": [
                "chutes"
              ]
            }
          }
        }
      }
    }
  }
}
